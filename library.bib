journal = {Journal of the American Statistical Association},
keywords = {1988,approach,blocked gibbs sampler,dirichlet prior,exponentially consistent test,generalized weighted chinese restaurant,identi cation,izenman som,mer,partition,presented compelling evidence showing,uniformly,viewed a nonparametric,weighted bayes factor},
