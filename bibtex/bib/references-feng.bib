@article{lindsten2017divideandconquer,
  title = {Divide-and-{{Conquer With Sequential Monte Carlo}}},
  author = {Lindsten, F. and Johansen, A. M. and Naesseth, C. A. and Kirkpatrick, B. and SchÃ¶n, T. B. and Aston, J. A. D. and Bouchard-CÃ´tÃ©, A.},
