@online{zadeh2016matrix,
  title = {Matrix {{Computations}} and {{Optimization}} in {{Apache Spark}}},
  author = {Zadeh, Reza Bosagh and Meng, Xiangrui and Staple, Aaron and Yavuz, Burak and Pu, Li and Venkataraman, Shivaram and Sparks, Evan and Ulanov, Alexander and Zaharia, Matei},
  date = {2016-07-12},
