@article{SanguriK2023IntermittentDemand,
  title = {Intermittent Demand, Inventory Obsolescence, and Temporal Aggregation Forecasts},
  author = {Sanguri, Kamal and Patra, Sabyasachi and Nikolopoulos, Konstantinos and Punia, Sushil},
