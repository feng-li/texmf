@article{WitkowskiJ2023IncentiveCompatibleForecasting,
  title = {Incentive-Compatible Forecasting Competitions},
  author = {Witkowski, Jens and Freeman, Rupert and Vaughan, Jennifer Wortman and Pennock, David M. and Krause, Andreas},
  date = {2023-03},
  journaltitle = {Management Science},
  volume = {69},
  number = {3},
  pages = {1354--1374},
  publisher = {INFORMS},
  issn = {0025-1909},
  doi = {10.1287/mnsc.2022.4410},
  url = {https://pubsonline.informs.org/doi/10.1287/mnsc.2022.4410},
  urldate = {2023-09-21},
  abstract = {We initiate the study of incentive-compatible forecasting competitions in which multiple forecasters make predictions about one or more events and compete for a single prize. We have two objectives: (1) to incentivize forecasters to report truthfully and (2) to award the prize to the most accurate forecaster. Proper scoring rules incentivize truthful reporting if all forecasters are paid according to their scores. However, incentives become distorted if only the best-scoring forecaster wins a prize, since forecasters can often increase their probability of having the highest score by reporting more extreme beliefs. In this paper, we introduce two novel forecasting competition mechanisms. Our first mechanism is incentive compatible and guaranteed to select the most accurate forecaster with probability higher than any other forecaster. Moreover, we show that in the standard single-event, two-forecaster setting and under mild technical conditions, no other incentive-compatible mechanism selects the most accurate forecaster with higher probability. Our second mechanism is incentive compatible when forecasters’ beliefs are such that information about one event does not lead to belief updates on other events, and it selects the best forecaster with probability approaching one as the number of events grows. Our notion of incentive compatibility is more general than previous definitions of dominant strategy incentive compatibility in that it allows for reports to be correlated with the event outcomes. Moreover, our mechanisms are easy to implement and can be generalized to the related problems of outputting a ranking over forecasters and hiring a forecaster with high accuracy on future events. This paper was accepted by Yan Chen, behavioral economics and decision analysis. Funding: This work was supported by the European Research Council [Grant ERC StG 307036] and the National Science Foundation [Grant CCF-1445755].},
  language = {en},
  keywords = {artificial intelligence,economics: game theory and bargaining,forecasting},
  file = {/home/fli/carbon/Papers/zotero-files/2023/Witkowski_Freeman_Vaughan_Pennock_Krause_2023_Incentive-Compatible Forecasting Competitions.pdf}
}

@article{HousseinEH2021DeepMachine,
  title = {Deep and Machine Learning Techniques for Medical Imaging-Based Breast Cancer: A Comprehensive Review},
  shorttitle = {Deep and Machine Learning Techniques for Medical Imaging-Based Breast Cancer},
  author = {Houssein, Essam H. and Emam, Marwa M. and Ali, Abdelmgeid A. and Suganthan, Ponnuthurai Nagaratnam},
  date = {2021-04-01},
  journaltitle = {Expert Systems with Applications},
  shortjournal = {Expert Systems with Applications},
