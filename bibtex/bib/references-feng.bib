@online{XuM2018VarianceReduction,
  title = {Variance Reduction Properties of the Reparameterization Trick},
  author = {Xu, Ming and Quiroz, Matias and Kohn, Robert and Sisson, Scott A.},
  date = {2018-12-27},
  eprint = {1809.10330},
  eprinttype = {arXiv},
