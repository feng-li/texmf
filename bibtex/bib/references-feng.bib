@article{Fruhwirth-SchnatterS2023GeneralizedCumulative,
  title = {Generalized Cumulative Shrinkage Process Priors with Applications to Sparse Bayesian Factor Analysis},
  author = {Frühwirth-Schnatter, Sylvia},
  date = {2023-03-27},
  journaltitle = {Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
  volume = {381},
