@article{bonifati2022time2feat,
  title = {{{Time2Feat}}: Learning Interpretable Representations for Multivariate Time Series Clustering},
  shorttitle = {{{Time2Feat}}},
  author = {Bonifati, Angela and Buono, Francesco Del and Guerra, Francesco and Tiano, Donato},
  date = {2022-11-23},
  journaltitle = {Proceedings of the VLDB Endowment},
