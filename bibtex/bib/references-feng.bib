@article{rosenblatt2016optimality,
  title = {On the {{Optimality}} of {{Averaging}} in {{Distributed Statistical Learning}}},
  author = {Rosenblatt, Jonathan and Nadler, Boaz},
  date = {2016-12},
  journaltitle = {Information and Inference},
  shortjournal = {Information and Inference},
  volume = {5},
  number = {4},
  eprint = {1407.2724},
  eprinttype = {arxiv},
  eprintclass = {math, stat},
  pages = {379--404},
  issn = {2049-8764, 2049-8772},
  doi = {10.1093/imaiai/iaw013},
  url = {http://arxiv.org/abs/1407.2724},
  urldate = {2022-09-29},
  abstract = {A common approach to statistical learning with big-data is to randomly split it among \$m\$ machines and learn the parameter of interest by averaging the \$m\$ individual estimates. In this paper, focusing on empirical risk minimization, or equivalently M-estimation, we study the statistical error incurred by this strategy. We consider two large-sample settings: First, a classical setting where the number of parameters \$p\$ is fixed, and the number of samples per machine \$n\textbackslash to\textbackslash infty\$. Second, a high-dimensional regime where both \$p,n\textbackslash to\textbackslash infty\$ with \$p/n \textbackslash to \textbackslash kappa \textbackslash in (0,1)\$. For both regimes and under suitable assumptions, we present asymptotically exact expressions for this estimation error. In the fixed-\$p\$ setting, under suitable assumptions, we prove that to leading order averaging is as accurate as the centralized solution. We also derive the second order error terms, and show that these can be non-negligible, notably for non-linear models. The high-dimensional setting, in contrast, exhibits a qualitatively different behavior: data splitting incurs a first-order accuracy loss, which to leading order increases linearly with the number of machines. The dependence of our error approximations on the number of machines traces an interesting accuracy-complexity tradeoff, allowing the practitioner an informed choice on the number of machines to deploy. Finally, we confirm our theoretical analysis with several simulations.},
  language = {en},
  keywords = {Mathematics - Statistics Theory,Statistics - Machine Learning},
  file = {/home/fli/carbon/Papers/zotero-files/2016/Rosenblatt_Nadler_2016_On the Optimality of Averaging in Distributed Statistical Learning.pdf}
}

