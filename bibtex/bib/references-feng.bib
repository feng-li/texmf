  keywords = {Bayesian statistics,judgmental forecasting,partial information,Shapley value,wisdom of crowds},
  file = {/home/fli/carbon/Papers/zotero-files/2021/Satopaa_Salikhov_Tetlock_Mellers_2021_Bias, Information, Noise.pdf}
}

