@article{huang2014timevariant,
  title = {Time-Variant Clustering Model for Understanding Cell Fate Decisions},
  author = {Huang, Wei and Cao, Xiaoyi and Biase, Fernando H. and Yu, Pengfei and Zhong, Sheng},
