@article{PetropoulosF2014HorsesCourses,
  title = {‘Horses for Courses’ in Demand Forecasting},
  author = {Petropoulos, Fotios and Makridakis, Spyros and Assimakopoulos, Vassilios and Nikolopoulos, Konstantinos},
  date = {2014-08-16},
  journaltitle = {European Journal of Operational Research},
  shortjournal = {European Journal of Operational Research},
  volume = {237},
  number = {1},
  pages = {152--163},
  issn = {0377-2217},
  doi = {10.1016/j.ejor.2014.02.036},
  url = {https://www.sciencedirect.com/science/article/pii/S0377221714001714},
  urldate = {2023-02-10},
  abstract = {Forecasting as a scientific discipline has progressed a lot in the last 40years, with Nobel prizes being awarded for seminal work in the field, most notably to Engle, Granger and Kahneman. Despite these advances, even today we are unable to answer a very simple question, the one that is always the first tabled during discussions with practitioners: “what is the best method for my data?”. In essence, as there are horses for courses, there must also be forecasting methods that are more tailored to some types of data, and, therefore, enable practitioners to make informed method selection when facing new data. The current study attempts to shed light on this direction via identifying the main determinants of forecasting accuracy, through simulations and empirical investigations involving 14 popular forecasting methods (and combinations of them), seven time series features (seasonality, trend, cycle, randomness, number of observations, inter-demand interval and coefficient of variation) and one strategic decision (the forecasting horizon). Our main findings dictate that forecasting accuracy is influenced as follows: (a) for fast-moving data, cycle and randomness have the biggest (negative) effect and the longer the forecasting horizon, the more accuracy decreases; (b) for intermittent data, inter-demand interval has bigger (negative) impact than the coefficient of variation; and (c) for all types of data, increasing the length of a series has a small positive effect.},
  language = {en},
  keywords = {Forecasting accuracy,Forecasting methods,M-Competitions,Simulation,Time series methods},
  file = {/home/fli/carbon/Papers/zotero-files/2014/Petropoulos_Makridakis_Assimakopoulos_Nikolopoulos_2014_‘Horses for Courses’ in demand forecasting2.pdf}
}

