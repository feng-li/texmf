@article{kerr2018optimization,
  title = {Optimization by {{Adaptive Stochastic Descent}}},
  author = {Kerr, Cliff C. and Dura-Bernal, Salvador and Smolinski, Tomasz G. and Chadderdon, George L. and Wilson, David P.},
  editor = {Kaderali, Lars},
