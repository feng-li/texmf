@thesis{LiLiMianXiangFuZaShiJianXuLieDeYuCeMoXingZuHeYanJiuJiYingYong,
  title = {面向复杂时间序列的预测模 型组合研究及应用},
  author = {{李莉}},
  institution = {北京航空航天大学},
  abstract = {在大数据时代，计算机技术的不断进步和时间序列数据的激增促进了时间序列预测研究的发展。有效的预测对于个人、企业乃至国家层面的决策都起着至关重要的作用。时间序列由于存在趋势性、季节性、周期性等特点，使得数据生成过程是有规律可循的，这为预测提供了可能性。但由于外部因素复杂多变，真实的时间序列数据不可避免的存在一定的随机性，这给预测任务带来了多源的复杂性。因此近几十年来，大量预测模型不断涌现。然而任何单一模型都有自身的缺 陷与不足，也都有各自不同的适用范围和条件，难以寻找到一种适用于所有数据 类型的预测方法。于是将单一模型进行组合的思想应运而生。预测组合在保证较 高预测精度的同时还能有效降低系统风险，因而该思想被广泛应用于时间序列预 测领域。 本文延续预测模型组合策略的思想，面向复杂时间序列，进一步研究模型组 合在解决不同预测难题中的贡献。时间序列的复杂性可以体现在以下几个方面： （1）时间序列模式随时间变化，即使是一条时间序列在不同的时间阶段也可能 存在截然不同的特征；（2）时间序列数据生成过程存在随机性和不确定性，特 别是在某些领域需求的产生是偶发的，存在大量零值且变化剧烈；（3）时间序 列数据通常根据其背景和特征进行分组，形成了横断面层次结构，预测结果应满 足分层结构的限制条件。本文面向上述时间序列预测中存在的挑战，建立预测组 合模型，以提升预测精度以及在决策管理中的应用价值。本文的主要研究内容及 结论如下： 1. 对于非平稳时间序列，统计特性随时间变化，因而可以基于时变特征构 造时变的预测组合权重，提出了时变特征驱动的密度预测模型组合框架。并通过 自动的贝叶斯变量选择方法衡量不同特征的重要性。与黑箱预测组合方案相比， 提出的方法具有更好的可解释性。最后应用于预测股票市场数据和 M3 竞赛数据， 验证了提出方法的有效性和优越性。 2. 面向间歇时间序列，对传统的预测组合方法进行了讨论和比较，并提出 了基于特征的间歇时间序列预测组合框架。考虑两类可以描述间歇时间序列的特 征，基于元学习模型，自动学习特征和组合权重的关系。提出的框架被应用于两 个真实数据集的预测，实验结果发现，提出的方法能提高点预测和分位数预测精 i度，为实际库存管理决策提供支持。 3. 在分层时间序列预测背景下，以死亡率预测为例进行研究，基于层次内 和层次间时间序列的相关关系，建立符合数据结构特点的预测模型。基于梯度提 升的思想，对死亡率预测进行建模，同时考虑年龄和空间层面的收缩。获得基础 预测后，利用调和预测组合方法进行调和，以满足死亡率分层结构的一致性。实 验结果发现，提出的方法实现了更高的整体预测准确性。},
  file = {/home/fli/.zotero/zotero-data/storage/R4LWI6WT/李莉 - 面向复杂时间序列的预测模 型组合研究及应用.pdf}
}

@article{NystrupP2020TemporalHierarchies,
  title = {Temporal Hierarchies with Autocorrelation for Load Forecasting},
  author = {Nystrup, Peter and Lindström, Erik and Pinson, Pierre and Madsen, Henrik},
  date = {2020-02-01},
  journaltitle = {European Journal of Operational Research},
  shortjournal = {European Journal of Operational Research},
  volume = {280},
  number = {3},
  pages = {876--888},
  issn = {0377-2217},
  doi = {10.1016/j.ejor.2019.07.061},
  url = {https://www.sciencedirect.com/science/article/pii/S037722171930640X},
  urldate = {2024-06-07},
  abstract = {We propose four different estimators that take into account the autocorrelation structure when reconciling forecasts in a temporal hierarchy. Combining forecasts from multiple temporal aggregation levels exploits information differences and mitigates model uncertainty, while reconciliation ensures a unified prediction that supports aligned decisions at different horizons. In previous studies, weights assigned to the forecasts were given by the structure of the hierarchy or the forecast error variances without considering potential autocorrelation in the forecast errors. Our first estimator considers the autocovariance matrix within each aggregation level. Since this can be difficult to estimate, we propose a second estimator that blends autocorrelation and variance information, but only requires estimation of the first-order autocorrelation coefficient at each aggregation level. Our third and fourth estimators facilitate information sharing between aggregation levels using robust estimates of the cross-correlation matrix and its inverse. We compare the proposed estimators in a simulation study and demonstrate their usefulness through an application to short-term electricity load forecasting in four price areas in Sweden. We find that by taking account of auto- and cross-covariances when reconciling forecasts, accuracy can be significantly improved uniformly across all frequencies and areas.},
  keywords = {Autocorrelation,Forecast combination,Forecasting,Reconciliation,Temporal aggregation},
  file = {/home/fli/.zotero/zotero-data/storage/EBJEJVC3/Nystrup et al. - 2020 - Temporal hierarchies with autocorrelation for load forecasting.pdf;/home/fli/.zotero/zotero-data/storage/XZL3JLWZ/1-s2.0-S037722171930640X-main.pdf}
}

